{
  "extends": [
    "@mskelton",
    "@mskelton/eslint-config/react",
    "plugin:local-rules/all"
  ],
  "plugins": ["local-rules"],
  "rules": {
    "react/no-unescaped-entities": "off"
  },
  "overrides": [
    {
      "files": "config/**",
      "rules": {
        "sort/object-properties": "off"
      }
    }
  ]
}
